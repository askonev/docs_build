#!/bin/bash

PWD=$(pwd)

source .env
source lib/launch_manager

## main configs
PORT=1000
JWT=false
PRIVATE_IP=true
NAME=unlim

docker run  -itd \
            -p $PORT:80 \
            --env JWT_ENABLED=$JWT \
            --env ALLOW_PRIVATE_IP_ADDRESS=$PRIVATE_IP \
            --restart unless-stopped \
            --name $NAME \
            onlyoffice/4testing-documentserver-ee:$VERSION

_wait_launch $NAME

cat ./lib/limits | docker exec -i $NAME bash
